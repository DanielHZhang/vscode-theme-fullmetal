{
	"name": "vscode-theme-fullmetal",
	"displayName": "Fullmetal Theme",
	"version": "0.1.0",
	"author": "Daniel Zhang",
	"description": "",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"onCommand:saveBuiltInSchemas"
	],
	"main": "build/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "saveBuiltInSchemas",
				"title": "Save Built-in VSCode Schemas"
			}
		],
		"themes": [
			{
				"label": "One Deep",
				"path": "./themes/fullmetal-color-theme.json",
				"uiTheme": "vs-dark"
			}
		]
	},
	"license": "MIT",
	"prettier": "@supersede/prettier-config",
	"eslintConfig": {
		"extends": "@supersede"
	},
	"scripts": {
		"watch": "concurrently \"npm:watch-*\"",
		"watch-generate": "nodemon --watch build build/theme.js",
		"watch-compile": "tsc-suppress --watch",
		"schemas": "tsc-suppress && node build/schema.js"
	},
	"devDependencies": {
		"@supersede/eslint-config": "^1.3.0",
		"@supersede/prettier-config": "^1.0.1",
		"@types/node": "^17.0.16",
		"@types/vscode": "^1.64.0",
		"concurrently": "^7.0.0",
		"eslint": "^8.10.0",
		"json-schema-to-typescript": "^10.1.5",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1",
		"tsc-suppress": "^1.0.7",
		"typescript": "^4.5.5"
	}
}
