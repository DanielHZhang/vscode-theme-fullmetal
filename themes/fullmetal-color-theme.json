  "semanticHighlighting": true,
  "semanticTokenColors": {
    "keyword.unsafe": "#ff4762",
    "macro": {
      "fontStyle": "italic"
    },
    "macroBang": {
      "fontStyle": "italic"
    }
  },
  "tokenColors": [
    {
      "name": "keywords",
      "scope": [
        "source keyword.other",
        "source keyword.control",
        "keyword.declaration",
        "keyword.operator.expression",
        "keyword.operator.new",
        "support.type.object.module",
        "variable.language.self",
        "variable.language.super",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#d44dae"
      }
    },
    {
      "name": "storage keyword (e.g. mod)",
      "scope": "storage.type",
      "settings": {
        "foreground": "#d44dae"
      }
    },
    {
      "name": "storage modifier (e.g. mut)",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#ff7e00"
      }
    },
    {
      "name": "attributes",
      "scope": "source meta.attribute",
      "settings": {
        "foreground": "#41a7fc"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "support.constant.property-value",
        "punctuation.definition.string",
        "punctuation.definition.char"
      ],
      "settings": {
        "foreground": "#8acd5b"
      }
    },
    {
      "name": "escape character",
      "scope": [
        "constant.character.escape",
        "punctuation.definition.template-expression.begin",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.type",
        "source keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ff4762"
      }
    },
    {
      "name": "comments",
      "scope": [
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#455573"
      }
    },
    {
      "name": "markdown code blocks",
      "scope": [
        "markup.fenced_code.block",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#455573"
      }
    },
    {
      "name": "bold",
      "scope": [
        "punctuation.definition.heading"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "markdown paragraph",
      "scope": [
        "meta.paragraph.markdown"
      ],
      "settings": {
        "foreground": "#dcbb"
      }
    },
    {
      "name": "variable declaration and parameters",
      "scope": [
        "variable.other",
        "variable.parameter",
        "variable.key.table"
      ],
      "settings": {
        "foreground": "#92d6cf"
      }
    },
    {
      "name": "struct/object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.resource.attribute",
        "entity.other.attribute-name",
        "punctuation.support.type.property-name",
        "variable.key",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "#c77ddd"
      }
    },
    {
      "name": "function",
      "scope": [
        "entity.name.function",
        "support.function",
        "markup.underline.link",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#41a7fc"
      }
    },
    {
      "name": "booleans, constants",
      "scope": [
        "constant.language.boolean",
        "constant.language",
        "support.constant",
        "constant.numeric",
        "punctuation.definition.list.begin",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#dd9046"
      }
    },
    {
      "name": "type definition",
      "scope": [
        "entity.name",
        "entity.other",
        "support.class.console",
        "support.type.primitive",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#efbd5d"
      }
    },
    {
      "name": "semicolon",
      "scope": [
        "keyword.operator.access.dot",
        "keyword.operator.namespace",
        "punctuation.semi"
      ],
      "settings": {
        "foreground": "#e1cfb1"
      }
    },
    {
      "name": "assignment operator (=)",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.borrow",
        "keyword.operator.key-value",
        "keyword.operator.dereference",
        "keyword.operator.type.annotation",
        "keyword.operator.arrow.skinny",
        "punctuation"
      ],
      "settings": {
        "foreground": "#3dacb4"
      }
    },
    {
      "name": "math oeprators",
      "scope": "keyword.operator.math",
      "settings": {
        "foreground": "#41a7fc"
      }
    }
  ]
}